                  mnemonic="N">
               <command
                     commandId="dev.menu.resource.commands.newProject"
                     style="push">
               </command>
               <separator
                     name="dev.separator1"
                     visible="true">
               </separator>
               <command
                     commandId="dev.menu.resource.commands.newDataItem"
                     style="push">
               </command>
               <command
                     commandId="dev.menu.resource.commands.newDll"
                     style="push">
               </command>
               <command
                     commandId="dev.menu.resource.commands.newRetCode"
                     style="push">
               </command>
               <separator
                     name="dev.separator2"
                     visible="true">
               </separator>
               <command
                     commandId="dev.menu.resource.commands.newServer"
                     style="push">
               </command>
               <command
                     commandId="dev.menu.resource.commands.newTrade"
                     style="push">
               </command>
               <command
                     commandId="dev.menu.resource.commands.newAop"
                     style="push">
               </command>
               <command
                     commandId="dev.menu.resource.commands.newTfm"
                     style="push">
               </command>
            </menu>
            <separator
