            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="alt+shift+P">
      </key>
      <key
            commandId="dev.menu.resource.commands.newDataItem"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="alt+shift+I">
      </key>
      <key
            commandId="dev.menu.resource.commands.newDll"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
